{
  "parser": "babel-eslint",
  "parserOptions" : {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "max-len": [2, 130, 2],
    "no-underscore-dangle": 0,
    "import/extensions": 0
  }
}
