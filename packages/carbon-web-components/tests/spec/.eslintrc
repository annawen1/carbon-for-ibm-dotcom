{
  "extends": [
    "../../src/.eslintrc"
  ],
  "globals": {
    "describe": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true,
    "it": true,
    "expect": true,
    "sinon": true
  },
  "rules": {
    "func-names": 0,
    "no-new": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": 0,
    "prefer-arrow-callback": 0
  }
}
