    "custom-event": "^1.0.0",
    "es6-promise": "^4.1.0",
    "lit-html": "^1.0.0"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "Subject does not match subject pattern!"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
